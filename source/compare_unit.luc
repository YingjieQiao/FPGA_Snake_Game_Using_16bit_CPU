module compare_unit (
    input alufn[6],
    input z,
    input v,
    input n,
    output out[16]
  ) {
sig s[16];
  always {
    s = 16b0;
    case(alufn[2:1]){
      b01: s[0] = z;
      b10: s[0] = n ^ v;
      b11: s[0] = z | (n ^ v);
      default: s[0] = b0;
    }
    out = s;
  }
}
